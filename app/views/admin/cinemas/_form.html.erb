<%= simple_form_for ['admin', @cinema] do |f| %>
    <%= f.input :name, label: 'Nom du cinéma'  %>
    <%= f.input :city, label: 'Ville' %>
    <%= f.input :address, label: 'Adresse' %>
    <%= f.input :zip_code, label: 'Code postal' %>
    <br />
    <%= f.button :submit, 'Valider la sélection' %>
<% end %>

